package gonzalez.salvador.tdd;

public class Rectangulo {

	// Creamos 2 atributos * altura y base
	public float altura;
	public float base;
	
	// Creamos el constructor de la clase
	public Rectangulo(float altura, float base) {
		this.setAltura(altura);
		this.setBase(base);
	}
	
	// creamos los getters & setters
	public float getAltura() {
		return altura;
	}
	public void setAltura(float altura) {
		this.altura = altura;
	}
	
	public float getBase() {
		return base;
	}
	public void setBase(float base) {
		this.base = base;
	}
	
	// creamos 2 metodos 1 para calcular el área de cualquier rectangulo 
	// y el segundo para observar la base y altura de cada rectangulo se llamara toString
	public String area() {
		return "El área del rectangulo es" + ;
	}
	
}
