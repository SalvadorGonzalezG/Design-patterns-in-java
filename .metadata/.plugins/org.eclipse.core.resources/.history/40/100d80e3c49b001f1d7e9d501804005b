package gonzalez.salvador.solid;

// Clase principal de nuestro programa
public class Main {
	public static void main(String[] args) {
		
		// instanciamos nuestro objeto Rectangulo
		Rectangulo rectangulo = new Rectangulo(10f, 5f);
		// Intanciamos nuestro objeto Triangulo
		Triangulo triangulo = new Triangulo(10f, 10f);
		// Instanciamos la clase presentacion
		Presentacion presentacion = new Presentacion();
		
		
		presentacion.area(rectangulo);
		
		presentacion.area(triangulo);
	}
}
